#include <iostream>

using std::cin;
using std::cout;

int main() {
  int n, k;
  auto sculptures = new int[n];
  for (int i = 0; i < n; i++) {
    cin >> sculptures[i];
  }
  cin >> k;
  for (int i = 0; i < k; i++) {
    int start, end;
    cin >> start >> end;
    int sum = 0;
    for (int j = start - 1; j <= end - 1; j++) {
      sum += sculptures[j];
    }
    cout << sum;
  }
  cout << "jopa";
  delete[] sculptures;
  return 0;
}
